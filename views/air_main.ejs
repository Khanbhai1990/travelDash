<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h3><%=airline[0].name%></h3>

    <table >

        <thead>
          <tr>
          <th>Airline</th>
          <th>description</th>
          <th>start</th>
          <th>destination</th>
          <th>delete</th>
        </tr>
        </thead>
    <tbody>
      <%for (let i=0;i<airline.length;i++){%>
      <tr >
        <td><%=airline[i].name%></td>
        <td><%=airline[i].description%></td>
        <td><%=airline[i].start%></td>
        <td><%=airline[i].destination%></td>
        <%console.log(airline[i])%>
        <td><button type="button" name="button"><a href="/deleteFlight/<%=airline[i].id%>">Delete</a></button></td>
      </tr>
        <%}%>
      </tbody>

    </table>

    <form action="/air_main" method="post">
      <input type="text" name="start" placeholder="start">
      <input type="text" name="destination" placeholder="destination">
    <input type="submit">
    </form>

    <button type="button" name="del"><a href="/logout">logout</a></button>


  </body>
</html>
